<div class="social-foo">
  <ul>
  {% if settings.instagram_link is not empty %}
    <li>
      <a href="{{ settings.instagram_link }}" target="_blank">
        <img src="{{ 'instagram.svg'| asset_url }}" alt="Instagram">
      </a>
    </li>
  {% endif %}
  {% if settings.facebook_link is not empty %}
    <li>
      <a href="{{ settings.facebook_link }}" target="_blank">
        <img src="{{ 'facebook.svg'| asset_url }}" alt="Facebook">
      </a>
    </li>
  {% endif %}
  {% if settings.twitter_link is not empty %}
    <li>
      <a href="{{ settings.twitter_link }}" target="_blank">
        <img src="{{ 'twitter.svg'| asset_url }}" alt="Twitter">
      </a>
    </li>
  {% endif %}
  {% if settings.pinterest_link is not empty %}
    <li>
      <a href="{{ settings.pinterest_link }}" target="_blank">
        <img src="{{ 'pinterest.svg'| asset_url }}" alt="Pinterest">
      </a>
    </li>
  {% endif %}
  {% if settings.google_link is not empty %}
    <li>
      <a href="{{ settings.google_link }}" target="_blank">
        <img src="{{ 'googleplus.svg'| asset_url }}" alt="G+">
      </a>
    </li>
  {% endif %}

  {% if settings.whatsapp_link is not empty %}
    <li>
      <a href="tel:{{ settings.whatsapp_link }}" target="_blank">
        <img src="{{ 'whatsapp.svg'| asset_url }}" alt="Whatsapp">
      </a>
    </li>
  {% endif %}
  </ul>
</div>
